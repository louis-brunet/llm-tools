{
  "name": "llm-tools",
  "packageManager": "yarn@4.7.0",
  "workspaces": [
    "packages/apps/*",
    "packages/libs/*"
  ],
  "scripts": {
    "build:server": "yarn workspaces foreach --recursive --topological-dev --from=cli-completions-server run build",
    "build:cli": "yarn workspaces foreach --recursive --topological-dev --from=cli-completions run build"
  },
  "devDependencies": {
    "@eslint/js": "^9.22.0",
    "@nestjs/cli": "^11.0.5",
    "@nestjs/schematics": "^11.0.2",
    "@types/node": "^22.13.10",
    "eslint": "^9.22.0",
    "eslint-config-prettier": "^10.1.1",
    "eslint-plugin-prettier": "^5.2.3",
    "globals": "^16.0.0",
    "prettier": "^3.5.3",
    "typescript": "^5.8.2",
    "typescript-eslint": "^8.26.1"
  },
  "$schema": "https://json.schemastore.org/package.json"
}
